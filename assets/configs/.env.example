# VMware Aria Suite Development Environment Variables
# Copy this file to .env and update with your actual values

# Database Configuration
POSTGRES_DB=aria_dev
POSTGRES_USER=aria_user
POSTGRES_PASSWORD=CHANGE_ME_SECURE_PASSWORD

# Grafana Configuration  
GRAFANA_PASSWORD=CHANGE_ME_SECURE_PASSWORD

# MinIO Configuration
MINIO_USER=minioadmin
MINIO_PASSWORD=CHANGE_ME_SECURE_PASSWORD

# API Server Configuration
JWT_SECRET=CHANGE_ME_SECURE_JWT_SECRET

# WARNING: Change all default passwords before deployment!
# Use strong, unique passwords for each service
# Consider using a password manager or secrets management system